 <%= form_for @income do |f| %> 
    <%= f.label :name %>
    <%= f.collection_select :user_ids, @users, :id, :username %>
    <%= f.label :Name_of_Income %>
    <%= f.text_field :name %>
    <%= f.label :Amount %>
    <%= f.number_field :amount %>
    <%= f.label :Description %>
    <%= f.text_field :description %>
    <%= f.submit %>
<% end %>

